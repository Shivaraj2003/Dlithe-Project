<?xml version="1.0" encoding="UTF-8"?>
 <element>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
    <display-name>Admin</display-name>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.example.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
   
    
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>com.example.registerServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    
    
     <servlet>
        <servlet-name>addBusServlet</servlet-name>
        <servlet-class>com.example.registerServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>addBusServlet</servlet-name>
        <url-pattern>/addBusServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>removeBusServlet</servlet-name>
        <servlet-class>com.example.registerServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>removeBusServlet</servlet-name>
        <url-pattern>/removeBusServlet</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>addTripServlet</servlet-name>
        <servlet-class>com.example.addTripServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>addTripServlet</servlet-name>
        <url-pattern>/addTripServlet</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>removeTripServlet</servlet-name>
        <servlet-class>com.example.registerServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>removeTripServlet</servlet-name>
        <url-pattern>/removeTripServlet</url-pattern>
    </servlet-mapping>
    
</web-app>
</element>
  